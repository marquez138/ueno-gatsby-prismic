{
  "name": "ueno-gatsby-starter",
  "description": "Opinionated Gatsby starter by Ueno",
  "scripts": {
  },
  "stack": "heroku-18",
  "env": {
    "NODE_ENV": {
      "description": "Environment to build",
      "required": false
    },
    "GATSBY_PRISMIC_ENDPOINT": {
      "description": "Prismic endpoint",
      "required": true
    },
    "PRISMIC_ACCESS_TOKEN": {
      "description": "Prismic access token",
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [

  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}
